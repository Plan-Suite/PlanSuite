@page "/account-lockout"
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<div class="container mb-5 mt-5">
    <h1 class="text-center"><i class="bi bi-lock"></i><br />@ViewData["Title"].ToString().ToUpper()</h1>
    <p class="text-muted text-center">This account has been locked out.<br />Please try again later.</p>
</div>
